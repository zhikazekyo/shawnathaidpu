
@{
    ViewBag.Title = "GoogleMap";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <style>
         html, body {
             height: 100%;
             margin: 0;
             padding: 0;
         }

        #map {
            width: 80%;
            height: 750px;
            top: 100%;
            left: 10%;
            background-color: grey;
        }
    </style>
        
</head>
<body id="page-top">
    <header class="masthead">
        <br><br><br><br><br>
        <a class="btn btn-xl">แผนที่สหกรณ์</a><br><br>
        <div id="map"></div>
        <script>
            var locations = [
                ['สหกรณ์ลาดปลาเค้า', 13.846876, 100.604481],
                ['หมู่บ้านอารียา', 13.847766, 100.605768],
                ['สปีดเวย์', 13.845235, 100.602711],
                ['สเต็ก ลุงหนวด', 13.862970, 100.613834]
            ];
            var map;
            function initMap() {
                map = new google.maps.Map(document.getElementById('map'), {
                    center: { lat: 13.870298, lng: 100.5485803 },
                    zoom: 15
                });
            }
        </script>
        <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBGWlbIKRjQV1WUujX7AlfU9VXgbZp-230&callback=initMap">

        </script>
        <br><br><br><br>
    </header>
</body>
</html>

