@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
<!DOCTYPE html>
<html lang="en">

<head>

</head>

<body id="page-top">

    <!-- Header -->
    <header class="masthead">
        <div class="container">
            <br><br><br><br><br><br>


            <div class="intro-heading"><font size=7>ลงทะเบียนสหกรณ์</font></div>

            <center>

                <form id="contactForm" name="sentMessage" novalidate>

                    <div class="form-group">

                        <div class="col-md-6">


                            <div class="form-group">
                                <input class="form-control" id="Coop_Name" name="Coop_Name" type="text" placeholder="ชื่อสหกรณ์ *" required data-validation-required-message="ได้โปรดกรอก ชื่อสหกรณ์.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="Coop_Latitude" name="Coop_Latitude" type="number" placeholder="ละติจูดของสหกรณ์*" required data-validation-required-message="ได้โปรดกรอก ละติจูดของสหกรณ์*.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="Coop_Longtitude" name="Coop_Longtitude" type="number" placeholder="ลองติจูดของสหกรณ์*" required data-validation-required-message="ได้โปรดกรอก ลองติจูดของสหกรณ์.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="Coop_Tel" name="Coop_Tel" type="tel" placeholder="เบอร์โทรศัพท์ของสหกรณ์*" required data-validation-required-message="ได้โปรดกรอก เบอร์โทรศัพท์ของสหกรณ์**.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="Coop_A_No" name="Coop_A_No" type="text" placeholder="บ้านเลขที่ *" required data-validation-required-message="ได้โปรดกรอก บ้านเลขที่.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="Coop_A_Sup" name="Coop_A_Sup" type="text" placeholder="ตำบล*" required data-validation-required-message="ได้โปรดกรอก ตำบล.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="Coop_A_District" name="Coop_A_District" type="text" placeholder="อำเภอ*" required data-validation-required-message="ได้โปรดกรอก อำเภอ*.">
                                <p class="help-block text-danger"></p>
                            </div>
                            
                            <div class="form-group">
                                <input class="form-control" id="AD_ID" name="AD_ID" type="text" placeholder="ชื่อผู้ดูเเล*" required data-validation-required-message="ได้โปรดกรอก ผู้ดูเเล*.">
                                <p class="help-block text-danger"></p>
                            </div>
                            
                            <div class="form-group">
                                <input class="form-control" id="Coop_A_Province" name="Coop_A_Province" type="text" placeholder="จังหวัด*" required data-validation-required-message="ได้โปรดกรอก จังหวัด*.">
                                <p class="help-block text-danger"></p>
                            </div>




                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <br>
                                <div id="success"></div>
                                <button id="sendMessageButton" class="btn btn-xl" type="submit">ลงทะเบียน</button>
                                <br><br><br><br><br><br><br><br><br><br><br><br><br>

                            </div>
                        </div>
                    </div>
                </form>
            </center>
        </div>

    </header>

</body>

</html>


}